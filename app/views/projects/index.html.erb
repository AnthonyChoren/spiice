<h1>Project#index</h1>
<p>Find me in app/views/project/index.html.erb</p>

<div class="container">
  <div class="row">
    <div class="col-sm-8 offset-sm-2">
      <div id="projects">
        <% @projects.each do |project| %>
          <h4><%= project.title %></h4>
          <p><%= project.description %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>
