        <div class="dribbble-card"></div>
        <div class="dribbble-card"></div>
        <div class="dribbble-card"></div>
      </div>
    </div>
  </div>
</div>


<% if @user.role == 'client' %>
<h3> this is a client profile </h3>
<% else %>
<h3> this is a designer profile </h3>
<% end %>

